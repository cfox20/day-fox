		"terminal.ansiBlack": "#3D2E28",
		"terminal.ansiRed": "#C75757",
		"terminal.ansiGreen": "#5F9B63",
		"terminal.ansiYellow": "#C4A058",
		"terminal.ansiBlue": "#5680A0",
		"terminal.ansiMagenta": "#9370A0",
